tal_price: subtotal,\n            full_name: full_name,\n            address_line1: address,\n            address_line2: apt,\n            city: city,\n            state: state,\n            postal_code: pincode,\n            country: 'US',\n            telephone: phone\n        };\n        dispatch(addOrder(params));\n        e.preventDefault();\n        dispatch(push('/thankyou'));\n    };\n\n    return (\n        <>\n            <div className=\"box\">\n                <p> Order your items </p>\n            </div>\n\n            <section className=\"details\">\n                <div className=\"product-details\">\n                    <h3>Shipment Details</h3>\n                    <h4>Please check your items and confirm it</h4>\n                    <div className=\"box2\">\n                        <table>\n                            {carts &&\n                                carts.map(cart => (\n                                    <tr>\n                                        <td className=\"td-item\">{cart.item.name}</td>\n                                        <td className=\"td-quantity\">{cart.quantity}</td>\n                                        <td className=\"td-price\">{cart.item.price}</td>\n                                    </tr>\n                                ))}\n                            <tr className=\"border\">\n                                <td id=\"total-price\">Total Price</td>\n                                <td id=\"item-count\">{totalitem}</td>\n                                <td id=\"subtotal\">${subtotal}</td>\n                            </tr>\n                        </table>\n                    </div>\n                </div>\n                <br />\n                <br />\n                <div className=\"dispatch\">\n                    <div className=\"form\">\n                        <label for=\"name\">Full name</label> <br />\n                        <input\n                            type=\"text\"\n                   